{
  "name": "exam_reviewer",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "nest build",
    "start": "dotenv -e .env -- node dist/main",
    "start:dev": "dotenv -e .env -- node dist/main",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "local": "nest start --watch",
    "build:prod": "NODE_ENV=production npm run build",
    "build:dev": "NODE_ENV=development npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.7",
    "@volcengine/openapi": "^1.32.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.2.3",
    "cookie-parser": "^1.4.6",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "mime-types": "^3.0.1",
    "openai": "^5.19.1",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "undici": "^7.8.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
